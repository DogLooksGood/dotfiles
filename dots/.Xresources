Xft.dpi: 192

! -------------------------------------------------------------
! URxvt Color Theme Start
! -------------------------------------------------------------

*.background: #171717
*.foreground: #A8A8A8

!black
*color0: #383c4a
*color8: #404552

!red
*color1: #9A4138
*color9: #E7766B

!green
*color2: #4BEC90
*color10: #3CBF75

!yellow
*color3: #D8D782
*color11: #CFCD63

!blue
*color4: #5294e2
*color12: #579CEF

!magenta
*color5: #CE50DD
*color13: #E7766B

!cyan
*color6: #6BE5E7
*color14: #90FDFF

!white
*color7: #CCCCCC
*color15: #FFFFFF

! -------------------------------------------------------------
! URxvt Color Theme End
! -------------------------------------------------------------

URxvt.scrollBar:         false
URxvt.scrollBar_right:   false

URxvt.transparent:       false

URxvt.depth: 32

URxvt.font: xft:unifont:pixelsize=32

! Disable bold font.
URxvt.boldFont:
URxvt.letterSpace: 0

! Add internal borders
URxvt.internalBorder: 20

Emacs.antialias:  true
*autohint:   true

URxvt.title: URxvt

!! URxvt Plugins
URxvt.perl-ext-common: default,matcher,resize-font,url-select,keyboard-select

!! Quick commands, dont rely on alias or any setup on the remote server.
URxvt.keysym.M-h: ls -lhb --color=auto\n
URxvt.keysym.C-M-g: grep --color=auto -r --include \\*.
URxvt.keysym.C-M-p: ps aux|grep --color=auto ""
URxvt.keysym.C-M-w: find . -name ""
URxvt.keysym.C-M-n: netstat -tulpn|grep --color=auto ""

! Disable ISO 14755 unicode input so we can use Ctrl-Shift bindings
URxvt.iso14755:        false
URxvt.iso14755_52:     false

! ! Disable Ctrl-Alt-c & Ctrl-Alt-v bindings (optional)
! URxvt.keysym.C-M-c:    builtin-string:
! URxvt.keysym.C-M-v:    builtin-string:

! Bind Ctrl-Shift-c & Ctrl-Shift-v to copy and paste
URxvt.keysym.C-S-0x43: eval:selection_to_clipboard
URxvt.keysym.C-S-0x56: eval:paste_clipboard

!! url-select
URxvt.keysym.M-u:	perl:url-select:select_next
URxvt.url-select.launcher: firefox
URxvt.url-select.underline: true
URxvt.matcher.button:	1

!! keyboard-select:
URxvt.keysym.M-Escape: perl:keyboard-select:activate

!! resize-font
URxvt.resize-font.smaller:	C-Down
URxvt.resize-font.bigger:	C-Up

Emacs.background: #211F23
Emacs.menuBar: off
Emacs.verticalScrollBars: off
Emacs.toolBar: off
Emacs.internalBorder: 16
Emacs.useXIM: off

!! Cursor size
Xcursor.size: 48
Xcursor.theme: Pop
