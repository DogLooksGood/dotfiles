Xft.dpi: 210

! -------------------------------------------------------------
! URxvt Color Theme Start
! -------------------------------------------------------------
URxvt.background: rgba:0000/2b00/3600/ef00
*foreground: #839496

!! black dark/light
*color0: #073642
*color8: #002b36

!! red dark/light
*color1: #dc322f
*color9: #cb4b16

!! green dark/light
*color2: #859900
*color10: #586e75

!! yellow dark/light
*color3: #b58900
*color11: #657b83

!! blue dark/light
*color4: #268bd2
*color12: #839496

!! magenta dark/light
*color5: #d33682
*color13: #6c71c4

!! cyan dark/light
*color6: #2aa198
*color14: #93a1a1

!! white dark/light
*color7: #eee8d5
*color15: #fdf6e3

! -------------------------------------------------------------
! URxvt Color Theme End
! -------------------------------------------------------------


URxvt*scrollBar:         false
URxvt*scrollBar_right:   false

URxvt*transparent:       false

URxvt*depth: 32

URxvt.font: xft:dejavu sans mono:pixelsize=30
URxvt.letterSpace: 0

URxvt.internalBorder:  40

*antialias:  true
*autohint:   true

!! URxvt Plugins
URxvt.perl-ext-common: default,matcher,resize-font,url-select,keyboard-select

!! url-select
URxvt.keysym.M-u:	perl:url-select:select_next
URxvt.url-select.launcher: firefox
URxvt.url-select.underline: true
URxvt.matcher.button:	1

!! keyboard-select:
URxvt.keysym.M-Escape: perl:keyboard-select:activate

!! resize-font
URxvt.resize-font.smaller:	C-Down
URxvt.resize-font.bigger:	C-Up

Emacs.menuBar: off
Emacs.verticalScrollBars: off
Emacs.toolBar: off
Emacs.internalBorder: 40

Polybar.background: #c0d0d0d0
Polybar.foreground: #000000
Polybar.highlight: #171717
Polybar.highlight.text: #ffffff

i3wm.highlight: #35c090
i3wm.highlight.text: #000000
i3wm.inactive: #171717

!! Cursor size
Xcursor.size: 54
