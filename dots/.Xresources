Xft.dpi: 210

! -------------------------------------------------------------
! URxvt Color Theme Start
! -------------------------------------------------------------

! special colors
URxvt.foreground:  #A8A8A8
URxvt.background:  #1A1A1A
URxvt.cursorColor:  #FFFFFF
!! black
*color0:      #000000
*color8:      #808080
!! red
*color1:      #D65946
*color9:      #D65946
!! green
*color2:      #6AAF50
*color10:     #6AAF50
!! yellow
*color3:      #BABA36
*color11:     #BABA36
!! blue
*color4:      #598BC1
*color12:     #598BC1
!! magenta
*color5:      #AB75C3
*color13:     #AB75C3
!! cyan
*color6:      #8FBC8F
*color14:     #8FBC8F
! white
*color7:      #d0d0d0
*color15:     #ffffff

! -------------------------------------------------------------
! URxvt Color Theme End
! -------------------------------------------------------------

URxvt.scrollBar:         false
URxvt.scrollBar_right:   false

URxvt.transparent:       false

URxvt.depth: 32

URxvt.font: xft:pt mono:pixelsize=28
! Disable bold font.
URxvt.boldFont:
URxvt.letterSpace: 0

URxvt.internalBorder: 30

*antialias:  true
*autohint:   true

!! URxvt Plugins
URxvt.perl-ext-common: default,matcher,resize-font,url-select,keyboard-select

!! quick ls
URxvt.keysym.M-h: ls -lh\n
URxvt.keysym.M-d: ls -lha\n
URxvt.keysym.Escape: 

!! url-select
URxvt.keysym.M-u:	perl:url-select:select_next
URxvt.url-select.launcher: firefox
URxvt.url-select.underline: true
URxvt.matcher.button:	1

!! keyboard-select:
URxvt.keysym.M-Escape: perl:keyboard-select:activate

!! resize-font
URxvt.resize-font.smaller:	C-Down
URxvt.resize-font.bigger:	C-Up

Emacs.background: #000000
Emacs.menuBar: off
Emacs.verticalScrollBars: off
Emacs.toolBar: off
Emacs.internalBorder: 16
Emacs.font: ibm plex mono-9

Polybar.border: #000000
Polybar.background: #EAEAEA
Polybar.foreground: #000000
Polybar.highlight: #3366AA
Polybar.highlight.text: #FFFFFF

bspwm.normal: #000000
bspwm.focused: #000000

i3wm.default: #333333
i3wm.highlight: #3366AA
i3wm.highlight.text: #171717
i3wm.inactive: #999999

!! Cursor size
Xcursor.size: 36
Xcursor.theme: redglass
